{
  "name": "@broid/utils",
  "version": "1.0.3",
  "main": "lib/index.js",
  "license": "AGPL-3.0+",
  "author": "Broid Team <opensource@broid.ai> (https://broid.ai)",
  "description": "Broid Utils used in Broid Integrations.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/broidHQ/integrations.git"
  },
  "bugs": {
    "url": "https://github.com/broidHQ/integrations/issues"
  },
  "homepage": "https://github.com/broidHQ/integrations/tree/master/broid-utils#readme",
  "keywords": [
    "broid",
    "activity",
    "strams",
    "activity streams 2",
    "messaging",
    "chat",
    "bot"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run copy:config && tsc",
    "clean": "rm -rf lib && rm -rf tsconfig.json && rm -rf tslint.json",
    "copy:config": "cp ../.ci/tsconfig.json . && cp ../.ci/tslint.json .",
    "lint": "npm run copy:config && tslint -c tslint.json src/**/*.ts",
    "lint:watch": "watch 'npm run lint' src",
    "nsp-check": "nsp check --warn-only -o summary",
    "test": "echo \"Error: no test specified\" && exit 1",
    "travis": "npm run lint && npm run nsp-check",
    "tsc:watch": "tsc --watch",
    "release": "npm run clean && npm run build && npm publish --access public",
    "watch": "concurrently --kill-others \"npm run lint:watch\" \"npm run tsc:watch\""
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "mmmagic": "^0.4.5",
    "pino": "^3.0.5",
    "ramda": "^0.22.1",
    "request": "^2.81.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.2",
    "@types/bluebird-global": "^3.5.1",
    "@types/node": "^7.0.12",
    "@types/node-uuid": "^0.0.28",
    "@types/ramda": "^0.0.5",
    "concurrently": "^3.1.0",
    "nsp": "^2.6.3",
    "tslint": "^4.3.1",
    "tslint-eslint-rules": "^3.5.1",
    "tslint-microsoft-contrib": "^4.0.1",
    "typescript": "^2.1.4",
    "watch": "^1.0.1"
  }
}
